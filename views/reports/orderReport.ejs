<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Order</title>
  <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css" />
  <!-- <script src="/js/bootstrap.js"></script> -->
  <link rel="stylesheet" href="src/css/allpage.css" />
</head>

<body style="background-color: #eeeeee" onload="fetchData()">
  <%- include("../components/navigation")%>
    <div class="div-second">
      <section class="d-flex justify-content-center py-3">
        <div class="card mx-5 mb-2 p-3" style="width: 67rem">
          <div>
            <span class="fs-2">Orders</span>
            <span class=" float-end">
              <input type="date" class="w-300 h-25 p-1" id="fromDate" onchange="searchdata()">
              <input type="date" class="w-300 h-25 p-1" id="toDate" onchange="searchdata()">
            </span>
          </div>
          <div>
            <table class="table table-hover">
              <thead>
                <tr id="productHeader"></tr>
              </thead>
              <tbody id="productData"></tbody>
            </table>
          </div>
        </div>
      </section>
      <%- include("../components/pagginationComponent")%>
    </div>
    <script src="/js/reports/orderReport.js"></script>
</body>

</html>