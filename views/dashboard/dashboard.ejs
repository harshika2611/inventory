<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css" />
    <script src="/js/bootstrap.js"></script>
    <link rel="stylesheet" href="src/css/allpage.css" />
    <link rel="stylesheet" href="src/css/dashboard/dashboardCustom.css" />
    <title>Dashboard</title>
  </head>

  <body>
    <%- include('../components/navigation')%>
    <section class="div-second dashboard-section">
      <div class="dashboard-container">
        <div class="div-container__grid">
          <div class="div-container__grid__salesoverview">
            <p class="div-container__grid__name">Sales Overview</p>
            <div class="salesoverview__todaysales">
              <div class="todaysales_div">
                <p class="salesoverview__p">Today Sales</p>
                <div class="todaysales_div_div">
                  <img
                    src="src/assets/dashboard/cart-shopping.svg"
                    alt="img"
                    width="30"
                    height="30"
                  />
                  <p>&#8377; 1000</p>
                </div>
              </div>
              <div class="todaysales_div">
                <p class="salesoverview__p">This Month Sales</p>
                <div class="todaysales_div_div">
                  <img
                    src="src/assets/dashboard/cart-shopping.svg"
                    alt="img"
                    width="30"
                    height="30"
                  />
                  <p>&#8377; 1000</p>
                </div>
              </div>
            </div>
          </div>

          <div class="div-container__grid__inventorysummary">
            <p class="div-container__grid__name">Inventory Summary</p>
            <div class="inventorysummary__totalproducts">
              <div class="totalproducts_div">
                <p class="inventorysummary__p">Total Products</p>
                <div class="totalproducts_div_div">
                  <img
                    src="src/assets/dashboard/Products.svg"
                    alt="img"
                    width="30"
                    height="30"
                  />
                  <p>100</p>
                </div>
              </div>
              <div class="outofstock_div">
                <p class="inventorysummary__p">Out Of Stock</p>
                <div class="outofstock_div_div">
                  <img
                    src="src/assets/dashboard/Products.svg"
                    alt="img"
                    width="30"
                    height="30"
                  />
                  <p>10</p>
                </div>
              </div>
            </div>
          </div>

          <div class="div-container__grid__purchaseoverview">
            <p class="div-container__grid__name">Purchase Overview</p>
            <div class="purchaseoverview__totalpurchase">
              <div class="totalpurchase_div">
                <p class="purchaseoverview__p">Total Prurchase</p>
                <div class="totalpurchase_div_div">
                  <img
                    src="src/assets/dashboard/purchase.svg"
                    alt="img"
                    width="30"
                    height="30"
                  />
                  <p>80</p>
                </div>
              </div>
              <div class="outofstock_div">
                <p class="purchaseoverview__p">Purchase Cost</p>
                <div class="outofstock_div_div">
                  <img
                    src="src/assets/dashboard/purchase.svg"
                    alt="img"
                    width="30"
                    height="30"
                  />
                  <p>&#8377; 10000</p>
                </div>
              </div>
            </div>
          </div>
          <div class="div-container__grid__latestpurchase">
            <div class="latestpurchase__div">
              <p class="div-container__grid__name">Latest Purchase</p>
              <a href="#">see all</a>
            </div>
            <table class="latestpurchase__table">
              <thead>
                <th>Purchase Id</th>
                <th>Supplier Name</th>
                <th>Date</th>
                <th>Amount</th>
                <th>Status</th>
              </thead>
              <tbody>
                <tr>
                  <td>1</td>
                  <td>ABC</td>
                  <td>01/02/2024</td>
                  <td>100</td>
                  <td>paid</td>
                </tr>
                <tr>
                  <td>1</td>
                  <td>ABC</td>
                  <td>01/02/2024</td>
                  <td>100</td>
                  <td>paid</td>
                </tr>
                <tr>
                  <td>1</td>
                  <td>ABC</td>
                  <td>01/02/2024</td>
                  <td>100</td>
                  <td>paid</td>
                </tr>
                <tr>
                  <td>1</td>
                  <td>ABC</td>
                  <td>01/02/2024</td>
                  <td>100</td>
                  <td>paid</td>
                </tr>
              </tbody>
            </table>
          </div>

          <div class="div-container__grid__lowquantitystock">
            <p class="div-container__grid__name">Low Quantity Stock</p>
            <table class="lowquantitystock__table">
              <thead>
                <th>Product Id</th>
                <th>Product Name</th>
                <th>Quantity</th>
              </thead>
              <tbody>
                <tr>
                  <td>1</td>
                  <td>ABC</td>
                  <td>100</td>
                </tr>
                <tr>
                  <td>1</td>
                  <td>ABC</td>
                  <td>100</td>
                </tr>
                <tr>
                  <td>1</td>
                  <td>ABC</td>
                  <td>100</td>
                </tr>
                <tr>
                  <td>1</td>
                  <td>ABC</td>
                  <td>100</td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </section>
  </body>
</html>
